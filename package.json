{
  "name": "forexaixchange",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "db:up": "docker compose -f backend/infra/docker-compose.yml up -d",
    "db:down": "docker compose -f backend/infra/docker-compose.yml down -v"
  },
  "workspaces": ["backend", "frontend"]
}
